{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/api/wiki", "dest": "/api/wiki.js" },
    { "src": "/api/ddg", "dest": "/api/ddg.js" },
    { "src": "/api/books", "dest": "/api/books.js" },
    { "src": "/api/github", "dest": "/api/github.js" },
    { "src": "/api/stack", "dest": "/api/stack.js" }
  ]
}
